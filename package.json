{
  "name": "where-is-my-stuff-backend",
  "version": "1.0.0",
  "description": "Is the back-end repository for the lend out app",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "tsc",
    "deploy": "firebase deploy",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "format": "npm run prettier && npm run lint:fix",
    "format:check": "npm run prettier:check && npm run lint",
    "test:unit": "jest --config jest.config.js",
    "test:unit:watch": "jest --config jest.config.js --watch",
    "test:unit:coverage": "jest --config jest.config.js --coverage",
    "test:unit:coverage:open": "npm run test:unit:coverage && open coverage/lcov-report/index.html"

  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:check && npm run test:unit:coverage"
    }
  },
  
  "keywords": [],
  "author": "",
  "license": "ISC",
