    "firebaseui-material-theme-library": "^5.0.0",
    "firebaseui-material-theme-library-styles": "^5.0.0",
    "firebaseui-material-theme-styles": "^5.0.0",
    "firebaseui-material-styles": "^5.0.0",
    "firebaseui-styles": "^5.0.0",
    "firebaseui-web": "^5.0.0",
    "firebaseui-web-library": "^5.0.0",
    "firebaseui-web-material": "^5.0.0",
    "firebaseui-web-material-library": "^5.0.0",
    "firebaseui-web-material-theme": "^5.0.0",
    "firebaseui-web-material-theme-library": "^5.0.0",
    "firebaseui-web-material-theme-library-styles": "^5.0.0",
    "firebaseui-web-material-theme-styles": "^5.0.0",
    "firebaseui-web-material-styles": "^5.0.0",
